---
import Layout from '../layouts/Layout.astro';
import Container from '../components/Container.astro';
import MainCallToAction from '../components/MainCallToAction.astro';
import IconCard from '../components/IconCard.astro';
import Features from '../components/Features.astro';
import Testimonials from '../components/Testimonials.astro';
import Stats from '../components/Stats.astro';
import CallToAction from '../components/CallToAction.astro';

// SEO-optimized content
const pageTitle = "Website Development Service in Bathurst and Orange | Professional Web Design";
const pageDescription = "Expert website development services in Bathurst and Orange. Professional web design, e-commerce solutions, and digital marketing. Get a free quote today!";
const pageKeywords = "website development bathurst, web design orange, website design bathurst, web development orange, e-commerce website bathurst, digital marketing bathurst orange, SEO bathurst, responsive web design, local web developer bathurst orange";
const canonicalUrl = "https://expresiv.com.au/website-development-bathurst-orange";

// Local business structured data
const structuredData = {
  "@context": "https://schema.org",
  "@type": "LocalBusiness",
  "name": "Expresiv Designs",
  "description": "Professional website development and web design services in Bathurst and Orange, NSW",
  "url": "https://expresiv.com.au",
  "telephone": "+61-2-1234-5678",
  "email": "info@expresiv.com.au",
  "address": {
    "@type": "PostalAddress",
    "addressLocality": "Bathurst",
    "addressRegion": "NSW",
    "addressCountry": "AU"
  },
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": "-33.4167",
    "longitude": "149.5806"
  },
  "areaServed": [
    {
      "@type": "City",
      "name": "Bathurst"
    },
    {
      "@type": "City", 
      "name": "Orange"
    }
  ],
  "serviceArea": {
    "@type": "GeoCircle",
    "geoMidpoint": {
      "@type": "GeoCoordinates",
      "latitude": "-33.4167",
      "longitude": "149.5806"
    },
    "geoRadius": "50000"
  },
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "Website Development Services",
    "itemListElement": [
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Website Design",
          "description": "Professional website design services for businesses in Bathurst and Orange"
        }
      },
      {
        "@type": "Offer", 
        "itemOffered": {
          "@type": "Service",
          "name": "E-commerce Development",
          "description": "Online store development and e-commerce solutions"
        }
      },
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service", 
          "name": "SEO Services",
          "description": "Search engine optimization to improve your website's visibility"
        }
      }
    ]
  }
};
---

<Layout title={pageTitle}>
  <head>
    <meta name="description" content={pageDescription} />
    <meta name="keywords" content={pageKeywords} />
    <link rel="canonical" href={canonicalUrl} />
    
    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <meta property="og:url" content={canonicalUrl} />
    <meta property="og:title" content={pageTitle} />
    <meta property="og:description" content={pageDescription} />
    <meta property="og:image" content="https://expresiv.com.au/images/web-development-bathurst-orange.jpg" />
    
    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image" />
    <meta property="twitter:url" content={canonicalUrl} />
    <meta property="twitter:title" content={pageTitle} />
    <meta property="twitter:description" content={pageDescription} />
    <meta property="twitter:image" content="https://expresiv.com.au/images/web-development-bathurst-orange.jpg" />
    
    <!-- Local SEO -->
    <meta name="geo.region" content="AU-NSW" />
    <meta name="geo.placename" content="Bathurst, Orange" />
    <meta name="geo.position" content="-33.4167;149.5806" />
    <meta name="ICBM" content="-33.4167, 149.5806" />
    
    <!-- Structured Data -->
    <script type="application/ld+json" set:html={JSON.stringify(structuredData)} />
    
    <!-- Additional SEO Meta Tags -->
    <meta name="robots" content="index, follow" />
    <meta name="author" content="Expresiv Designs" />
    <meta name="language" content="English" />
    <meta name="revisit-after" content="7 days" />
    <meta name="distribution" content="global" />
    <meta name="rating" content="general" />
  </head>

  <main class="space-y-40">
    <div class="relative" id="home">
      <div aria-hidden="true" class="absolute inset-0 grid grid-cols-2 -space-x-52 opacity-40 dark:opacity-20">
        <div class="blur-[106px] h-32 bg-gradient-to-r from-sky-700 to-sky-700"></div>
        <div class="blur-[106px] h-32 bg-gradient-to-r from-purple-700 to-fuchsia-600"></div>
      </div>

      <!-- Hero Section -->
      <section class="relative pt-[200px] pb-[120px]">
        <Container>
          <div class="flex flex-col md:flex-row items-center gap-5 md:gap-20">
            <div class="w-[100%]">
              <h1 class="lg:leading-relaxed leading-10 text-gray-900 dark:text-white font-bold text-4xl md:text-5xl xl:text-6xl w-[60%]">
                Professional Website Development Services in <span class="text-primary">Bathurst & Orange</span>
